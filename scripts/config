#config

INTERFACE=mon0
WIRELESS=wlan0
MODULE=ath9k

SUPPORTED_REAVER_VERSION="v1.4"

REAVER_OPTS="-a -vv"


WASH_PID="/tmp/autoreaver-wash.pid"
REAVER_PID="/tmp/autoreaver-reaver.pid"

TMP_DIR="/tmp"
WASH_OUTPUT="$TMP_DIR/autoreaver-wash.out"
REAVER_OUTPUT="$TMP_DIR/autoreaver-reaver.out"

WHITELIST_FILE="/tmp/autoreaver-whitelist"
RESULT_FILE="/tmp/autoreaver-result"

WASH_BIN="/usb/usr/bin/wash"
REAVER_BIN="/usb/usr/bin/reaver"
